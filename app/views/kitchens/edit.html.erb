<%= form_with(model: @kitchen_skill_maps, local: true) do |f| %>
  <%= f.label :conro %>
  <%= f.number_field :conro %>
  
  <%= f.label :oven %>
  <%= f.number_field :oven %>
  
  <%= f.label :flier %>
  <%= f.number_field :flier %>
  
  <%= f.label :salad %>
  <%= f.number_field :salad %>
  
  <%= f.label :drink %>
  <%= f.number_field :drink %>
  
  <%= f.submit "更新", class: "btn btn-primary btn-block btn-user--edit" %>
  
<% end %>