<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with(model: @kitchen_skill_map,  local: true) do |f| %>
  
      <%= f.label :conro %>
      <%= f.number_field :conro, class: "form-control", min: 0, max: 5 %>
  
      <%= f.label :oven %>
      <%= f.number_field :oven, class: "form-control", min: 0, max: 5 %>
  
      <%= f.label :flier %>
      <%= f.number_field :flier, class: "form-control", min: 0, max: 5 %>
  
      <%= f.label :salad %>
      <%= f.number_field :salad, class: "form-control", min: 0, max: 5 %>
  
      <%= f.label :drink %>
      <%= f.number_field :drink, class: "form-control", min: 0, max: 5 %>
  
      <%= f.submit "更新", class: "btn btn-primary btn-block btn-user--edit" %>
    <% end %>
  </div>
</div>
